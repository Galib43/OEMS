
@model Models.ViewModels.ParticipantEntryVm

@{
    Layout = "~/Views/Share/_LayoutShare.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="container">
            <h2 class="text-primary">@Model.Name</h2>
            <hr />
            <dl class="dl-horizontal">
                <dt>
                    @Html.DisplayNameFor(model => model.Organization.Name)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Organization.Name)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Course.Name)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Course.Name)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.ContactNo)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.ContactNo)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Email)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Email)
                </dd>

                <dt>
                    Address
                </dt>

                <dd>
                    @Model.FullAddress()
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.Country.Name)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.Country.Name)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.City.Name)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.City.Name)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.PostalCode)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.PostalCode)
                </dd>
            </dl>

            <hr />

            <p>
                @Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |
                @Html.ActionLink("Back to List", "Index")
            </p>
        </div>

    </section>
    <!-- /.content -->
</div>

