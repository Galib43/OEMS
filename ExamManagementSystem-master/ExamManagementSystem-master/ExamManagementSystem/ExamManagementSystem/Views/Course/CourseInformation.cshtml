@model Models.ViewModels.CourseEntryVm

@{
    Layout = "~/Views/Share/_LayoutShare.cshtml";
}

<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-12">
                <!-- Chat box -->
                <div class="box box-success">
                    <div class="box-header">
                        <h3 class="box-title">Course Update</h3>
                    </div>
                    <div class="box-body">
                        <!-- Form Start -->

                        <form method="post">
                            <div class="form-horizontal">
                                <div class="form-group">
                                    <label for="Organization" class="col-md-2 control-label">Organization</label>
                                    <div class="col-md-10">
                                        @Html.DropDownListFor(c => c.OrganizationId, Model.OrganizationListItem, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Name" class="col-md-2 control-label">Name</label>
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(c => c.Name, new { @class = "form-control", placeholder = "Name" })
                                        @Html.ValidationMessageFor(c => c.Name, null, new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Code" class="col-md-2 control-label">Code</label>
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(c => c.Code, new { @class = "form-control", placeholder = "Code" })
                                        @Html.ValidationMessageFor(c => c.Code, null, new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Duration" class="col-md-2 control-label">Course Duration (hrs)</label>
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(c => c.Duration, new { @class = "form-control", placeholder = "Course Duration" })
                                        @Html.ValidationMessageFor(c => c.Duration, null, new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Credit" class="col-md-2 control-label">Credit</label>
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(c => c.Credit, new { @class = "form-control", placeholder = "Credit" })
                                        @Html.ValidationMessageFor(c => c.Credit, null, new { @class = "text-danger" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Outline" class="col-md-2 control-label">Outline</label>
                                    <div class="col-md-10">
                                        @Html.TextAreaFor(c => c.Outline, new { @class = "form-control", placeholder = "Outline..", rows = "3" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label for="Tags" class="col-md-2 control-label">Tags</label>
                                    <div class="col-md-10">
                                        @Html.TextBoxFor(c => c.Tag, new { @class = "form-control", placeholder = "Tags" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <div class="col-md-offset-2 col-md-10">
                                        <button type="submit" class="btn btn-primary pull-right">Update</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <!--Form Closed-->
                    </div>
                </div>
            </section>
        </div>
        <!-- /.row (main row) -->

    </section>
    <!-- /.content -->
</div>
@section scripts{
    <script src="~/Scripts/AppScript/OrganizationEntry/OrganizationDetail.js"></script>

}


























































