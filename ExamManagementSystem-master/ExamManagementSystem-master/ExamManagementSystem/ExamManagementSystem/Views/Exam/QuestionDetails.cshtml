
@model Models.Question
@{
    Layout = "~/Views/Share/_LayoutShare.cshtml";
}


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Dashboard
            <small>Control panel</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
            <li class="active">Dashboard</li>
        </ol>
    </section>

    <!-- Main content -->
    <section class="content">
        <!-- Main row -->
        <div class="row">
            <!-- Left col -->
            <section class="col-lg-12">
                <!-- Chat box -->
                <div class="box box-success">
                    <div class="box-header">
                        <h3 class="box-title">Question Information</h3>
                    </div>
                    <div class="box-body">
                        <!-- Form Start -->
                        <div class="container">
                            <dl class="dl-horizontal">

                                <dt>
                                    @Html.DisplayNameFor(model => model.Name)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Name)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.Marks)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.Marks)
                                </dd>

                                <dt>
                                    @Html.DisplayNameFor(model => model.QuestionOptionses)
                                </dt>

                                <dd>
                                    @Html.DisplayFor(model => model.QuestionOptionses.Count)
                                </dd>

                            </dl>
                            <p>
                                @*@Html.ActionLink("Edit", "Edit", new { id = Model.Id }) |*@
                                @Html.ActionLink("Back to List", "QuestionIndex")
                            </p>
                        </div>

                        <!--Form Closed-->
                    </div>
                </div>
            </section>
        </div>
        <!-- /.row (main row) -->

    </section>
    <!-- /.content -->
</div>
@section scripts{
<script src="~/Scripts/AppScript/ExamEntry/ExamDetail.js"></script>
}
